{
  "name": "@vue-monaco/loader",
  "type": "module",
  "version": "0.0.2",
  "description": "the library aims to setup monaco editor into your browser",
  "author": "Kriszu <wangsizhu0504@outlook.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "loader",
    "monaco-editor"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "stub": "rollup -c"
  },
  "peerDependencies": {
    "monaco-editor": ">= 0.21.0"
  }
}
